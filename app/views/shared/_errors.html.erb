<div class="text-center">
<% if target.errors.any? %>
  <div class = 'failure'>
    <h2> <%= pluralize(target.errors.count, "error") %>
    prohibited this record from being saved: </h2>
  </div>

    <% target.errors.full_messages.each do |message| %>
      <%= message%> <br>
    <% end %>

<% end %>
</div>
